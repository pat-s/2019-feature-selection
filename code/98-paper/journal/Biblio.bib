  keywords = {remote sensing,earth observation,forest ecosystem,forest health,non-spectral traits (N-ST),spectral trait variations (STV),spectral traits (ST),traits},
  pages = {1029},
  file = {/home/pjs/Zotero/storage/LC8V74HS/Lausch et al. - 2016 - Understanding Forest Health with Remote Sensing -P.pdf;/home/pjs/Zotero/storage/ZJ9SV8FY/1029.html},
