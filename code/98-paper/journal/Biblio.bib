  pages = {440-451},
  file = {/home/pjs/Zotero/storage/DNNHAS33/Huete et al. - 1997 - A comparison of vegetation indices over a global s.pdf;/home/pjs/Zotero/storage/6KWSB8PJ/S0034425796001125.html},
