  doi = {10/f5q4b4},
  author = {Jiang, Yanbin and Wang, Tiejun and {de Bie}, C. A. J. M. and Skidmore, A. K. and Liu, Xuehua and Song, Shanshan and Zhang, Li and Wang, Jian and Shao, Xiaoming},
  month = mar,
  year = {2014},
  keywords = {Species distribution model,China,Climate,Macro-habitat,MaxEnt,Vegetation},
