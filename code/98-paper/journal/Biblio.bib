  month = jan,
  year = {2010},
  keywords = {classification,Ganoderma,hyperspectral reflectance,oil palm,partial least square,spectroscopy},
  pages = {734-747},
  file = {/home/pjs/Zotero/storage/IJUDWJ2W/Lelong et al. - 2010 - Evaluation of Oil-Palm Fungal Disease Infestation .pdf;/home/pjs/Zotero/storage/7BW5NKZH/htm.html},
